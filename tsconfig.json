{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "baseUrl": ".",
        "resolveJsonModule": true,
        "paths": {
            "@pages/*":["page-objects/*"],
            "@utils/*":["helpers/*"],
            "@fixtures":["fixtures/fixtures.ts"],
